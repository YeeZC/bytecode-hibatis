<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibatis SYSTEM "hibatis.dtd">

<hibatis id="me.zyee.hibatis.bytecode.TestDao" entity="me.zyee.hibatis.bytecode.TestEntity">
    <map id="all">
        <column column="id" field="id" javaType="java.lang.String"/>
        <column column="name" field="name" javaType="java.lang.String"/>
    </map>
    <select id="findAll">
        from TestEntity
    </select>
    <select id="findNativeAll" native="true" resultMap="all">
        select * from test
    </select>
    <select id="findById">
        from TestEntity where id = :id
    </select>

    <insert id="insert">
        insert into test values (:id, :name)
    </insert>
</hibatis>
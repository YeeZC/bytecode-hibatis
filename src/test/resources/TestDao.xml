<dao id="me.zyee.hibatis.bytecode.TestDao" entity="me.zyee.hibatis.bytecode.TestEntity">
    <select id="findAll">
        from TestEntity
    </select>
    <select id="findNativeAll" native="true">
        select * from test
    </select>
    <select id="findById">
        from TestEntity where id = :id
    </select>

    <insert id="insert">
        insert into test values (:id, :name)
    </insert>
</dao>